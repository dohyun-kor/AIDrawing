<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.example.model.dao.MyFurnitureDao">

    <!-- 가구 전시를 위한 insert 쿼리-->
    <insert id="insertFurnitureDisplay" parameterType="java.util.Map">
        INSERT INTO MyFurniture (user_id, fur_id)
        VALUES (#{userId}, #{myFurniture.furId})
    </insert>


</mapper>